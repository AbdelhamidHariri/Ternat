{
  "name": "ternat",
  "version": "1.0.1",
  "description": "A package to make database migration easy",
  "main": "./src/index.js",
  "bin": {
    "ternat": "./dist/cjs/index.js"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdelhamidHariri/Ternat.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "database",
    "migrate",
    "node",
    "migrations"
  ],
  "author": "Abed01-lab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AbdelhamidHariri/Ternat/issues"
  },
  "homepage": "https://github.com/AbdelhamidHariri/Ternat#readme",
  "dependencies": {
    "@types/pg": "8.11.5",
    "chalk": "4.0.0",
    "commander": "13.1.0",
    "dotenv": "16.4.5",
    "pg": "8.11.3"
  },
  "devDependencies": {
    "semantic-release": "23.0.8"
  }
}
